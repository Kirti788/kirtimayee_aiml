    {
      "cell_type": "code",
      "source": [
        "base =int(input('enter the base number = '))\n",
        "exponent =int(input('enter the exponent = '))\n",
        "\n",
        "result = 1\n",
        "\n",
        "for exponent in range(exponent, 0, -1):\n",
        "    result *= base\n",
        "\n",
        "print(\"Answer = \" + str(result))"
