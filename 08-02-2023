      "source": [
        "f=int(input('enter the first number = '))\n",
        "s=int(input('enter the second number = '))\n",
        "n=int(input('enter the number of sequence for the fibonacci series = '))\n",
        "print('the fibonacci series = ')\n",
        "print(f)\n",
        "print(s)\n",
        "for i in range(f,n):\n",
        "  if(i<=1):\n",
        "    result=i\n",
        "  else:\n",
        "    result=f+s\n",
        "    f=s\n",
        "    s=result\n",
        "  print(result)\n",
