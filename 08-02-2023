      ],
      "metadata": {
        "id": "nkTzQ8EDLqNR"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "def convertTooctal(n):\n",
        "   if n > 1:\n",
        "       convertTooctal(n//8)\n",
        "   print(n % 8,end = '')\n",
        "\n",
        "dec =int(input('enter the number in decimal : '))\n",
        "\n",
        "convertTooctal(dec)\n",
        "print('  in octal')\n"
      ],
